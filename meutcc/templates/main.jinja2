<!DOCTYPE HTML>
<html lang="{{ request.locale_name}}">
<head>
    {% block head %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"  href="{{ request.static_url('meutcc:static/css/main.css') }}"/>
    <title>Template</title>
    {% endblock %}
</head>
<body>
    <!--  -->
    <div id="conteudo">
        <div id="cabeçalho">
            <p>
                <img id="imglogo" src="{{request.static_url('meutcc:static/imagens/tccimage.jpeg')}}"  alt="pyramid web framework">
            </p>
        </div>
        <div id="navbar">
            <ul>
                <li><a href="{{ request.route_url('home')}}">Home</a></li>
                <li><a href="{{ request.route_url('lote_venda')}}">Lotes vendas</a></li>
                <li><a href="{{ request.route_url('transacao')}}" name="t">Transações</a></li>
                <li><a href="{{ request.route_url('lote_parcelado')}}" name="lp">Lotes parcelados futuros</a></li>
                <li><a href="{{ request.route_url('icpt')}}">Icpt</a></li>
            </ul>
        </div>
        {% block content %}{% endblock %}
    </div>
    <div id="rodape">
        © Anderson Marques Morais
    </div>
</body>
</html>